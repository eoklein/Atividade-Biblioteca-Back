datasource db {
  provider = "sqlite"
  url      = "file:biblioteca.db"
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id Int @id @default(autoincrement())
  nome String
  senha String
  isAdmin  Boolean @default(false)
}

model Books {
  id Int @id @default(autoincrement())
  titulo String
  autor String
  available Boolean @default(true)
}